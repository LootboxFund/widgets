  "scripts": {
    "extract": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file ./src/lib/lang/extracted/en.json",
    "compile": "formatjs compile './src/lib/lang/extracted/en.json' --out-file ./src/lib/lang/compiled/en.json",
    "codegen": "graphql-codegen --config codegen.yml",
    "watch": "rollup -c -w",
    "build": "yarn extract && yarn compile && NODE_ENV=production rollup -c",
    "test": "jest --verbose --watch",
    "storybook": "NODE_ENV=dev start-storybook",
    "build-storybook": "build-storybook",
    "pipedream:sample:onLootboxUri": "node scripts/sample-lootboxUri.js",
    "deploy:prod": "NODE_ENV=production rollup -c && NODE_ENV=production node scripts/deploy-widgets.js",
    "deploy:cassini": "NODE_ENV=production node scripts/deploy-authentication.js",
    "deploy:euler": "NODE_ENV=production node scripts/deploy-my-profile.js",
    "deploy:tangris": "NODE_ENV=production node scripts/deploy-battle-feed.js",
    "deploy:degrasse": "NODE_ENV=production node scripts/deploy-lootbox-feed.js",
    "deploy:publicprofile": "NODE_ENV=production node scripts/deploy-public-profile.js",
    "deploy:viralonboarding": "NODE_ENV=production node scripts/deploy-viral-onboarding.js",
    "deploy:authanon": "NODE_ENV=production node scripts/deploy-authenticate-anon-users.js",
    "deploy:signout": "NODE_ENV=production node scripts/deploy-sign-out.js",
    "deploy:mytickets": "NODE_ENV=production node scripts/deploy-my-tickets.js",
    "deploy:cosmiclootbox": "NODE_ENV=production node scripts/deploy-cosmic-lootbox.js",
    "deploy:navbar": "NODE_ENV=production node scripts/deploy-nav-bar.js",
    "deploy:nerds": "yarn deploy:prod && yarn deploy:cassini && yarn deploy:euler && yarn deploy:tangris && yarn deploy:degrasse && yarn deploy:publicprofile && yarn deploy:viralonboarding && yarn deploy:authanon && yarn deploy:signout && yarn deploy:mytickets && yarn deploy:cosmiclootbox && yarn deploy:navbar",
    "deploy:hostingwidgets": "yarn deploy:euler && yarn deploy:publicprofile && yarn deploy:signout && yarn deploy:viralonboarding && yarn deploy:navbar && yarn deploy:cosmiclootbox && yarn deploy:authanon && yarn deploy:mytickets",
    "logrocket:upload": "bash ./logrocket-deploy.sh",
    "sandbox": "surge --domain=https://______.surge.sh"
  },
